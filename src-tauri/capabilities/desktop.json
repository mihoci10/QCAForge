{
  "identifier": "desktop-capability",
  "platforms": [
    "macOS",
    "windows",
    "linux"
  ],
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-set-title",
    "clipboard-manager:allow-write-text",
    "clipboard-manager:allow-read-text",
    "clipboard-manager:allow-clear",
    "clipboard-manager:default",
    "dialog:default",
    "fs:default",
    "fs:write-all",
    "fs:read-all"
  ]
}